<div class="contact_list">
	<table>
		<h1>Contact List</h1>
		<thead>
			<tr>
				<th></th>
				<th>First name</th>
				<th>Last name</th>
				<th>Email</th>
				<th>Note</th>
			</tr>
		</thead>
		<tbody>
			<% $rolodex.contacts.each do |contact| %>			
				<tr>
					<td><a href="/contacts/<%= contact.id %>/edit">EDIT</a></td>
					<td><a href="/contacts/<%= contact.id %>"><%= contact.first_name %></a></td>
					<td><%= contact.last_name %></td> 
					<td><%= contact.email %></td> 
					<td><%= contact.note %></td> 
				</tr>
			<% end %>
		</tbody>
	
	</table>
</div> 